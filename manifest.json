{
  "manifest_version": 2,
  "name": "mailman plus",
  "version":"0.1.2",
  "description": "Give you a better  mailman list browse experience",
  "icons":
  {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/pipermail/*"],
      "css": ["styles.css"],
      "js": ["mail.js"]
    }
  ]
}